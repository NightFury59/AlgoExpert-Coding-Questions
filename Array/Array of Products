// O(n) time | O(m) space
function arrayOfProducts(array) {
  let products = new Array(array.length).fill(1);
  let left = new Array(array.length).fill(1);
  let right = new Array(array.length).fill(1);
  
  let leftRunningProducts = 1;
  for (let i = 0; i < array.length; i++) {
    left[i] = leftRunningProducts;
    leftRunningProducts *= array[i];
  }

  let rightRunningProducts = 1;
  for (let i = array.length - 1; i >= 0; i--) {
    right[i] = rightRunningProducts;
    rightRunningProducts *= array[i];
  }

  for (let i = 0; i < array.length; i++) {
    products[i] = left[i] * right[i];
  }
  return products
}

// Do not edit the line below.
exports.arrayOfProducts = arrayOfProducts;
